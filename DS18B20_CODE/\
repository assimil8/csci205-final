#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

int main(){
	int c;
	FILE *file;

	//essential commands
	system("sudo modprobe w1-gpio");
	system("sudo modprobe w1-therm");
	//output temp read
	system("more /sys/bus/w1/devices/*/w1_slave > output.txt");
	file = fopen("output.txt","r");
	if (!file) 
		return 1;
	for (c=1;c<=49;c++)
		fprintf(file,"%d\n",c);
	fclose(file);
	return 0;
}

